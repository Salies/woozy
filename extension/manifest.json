{
    "manifest_version": 2,
    "name": "woozy",
    "version": "1.0",
  
    "description": "Extracts HLTV Fantasy data.",

    "permissions": [
        "scripting",
        "activeTab",
        "storage"
    ],
  
    "icons": {
      "48": "icons/woozy.png"
    },

    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_title": "Extract data"
    }
  }
  